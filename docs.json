{
	"$schema": "https://mintlify.com/docs.json",
	"theme": "mint",
	"name": "BookingShake API",
	"colors": {
		"primary": "#FFCD00",
		"light": "#FFD633",
		"dark": "#E6BA00"
	},
	"favicon": "/favicon.ico",
	"navigation": {
		"tabs": [
			{
				"tab": "Documentation",
				"groups": [
					{
						"group": "Getting Started",
						"pages": [
							"api-reference/introduction",
							"api-reference/authentication",
							"api-reference/rate-limiting",
							"api-reference/quickstart"
						]
					},
					{
						"group": "Webhooks",
						"pages": [
							"api-reference/webhooks/introduction",
							"api-reference/webhooks/security"
						]
					},
					{
						"group": "Guides",
						"pages": [
							"api-reference/errors",
							"api-reference/use-cases"
						]
					}
				]
			},
			{
				"tab": "API Reference",
				"openapi": "/api-reference/bookingshake-openapi.json",
				"groups": [
					{
						"group": "Events",
						"pages": [
							"POST /events/create"
						]
					},
					{
						"group": "Resources",
						"pages": [
							"GET /sources",
							"GET /spaces",
							"GET /status"
						]
					},
					{
						"group": "Fields",
						"pages": [
							"GET /fields"
						]
					}
				]
			}
		],
		"global": {
			"anchors": [
				{
					"anchor": "Website",
					"href": "https://bookingshake.com",
					"icon": "globe"
				},
				{
					"anchor": "Dashboard",
					"href": "https://crm.bookingshake.com",
					"icon": "browser"
				}
			]
		}
	},
	"logo": {
		"light": "/logo/bookingshake_logo.png",
		"dark": "/logo/bookingshake_logo.png"
	},
	"navbar": {
		"links": [
			{
				"label": "Support",
				"href": "mailto:support@bookingshake.com"
			}
		],
		"primary": {
			"type": "button",
			"label": "Dashboard",
			"href": "https://crm.bookingshake.com"
		}
	},
	"api": {
		"baseUrl": "https://api.bookingshake.io/api",
		"auth": {
			"method": "bearer"
		}
	},
	"contextual": {
		"options": ["copy", "view", "chatgpt", "claude", "perplexity", "mcp", "cursor", "vscode"]
	},
	"footer": {
		"socials": {
			"linkedin": "https://linkedin.com/company/bookingshake"
		}
	}
}
